<html>

<head>
<link rel="stylesheet" href="/css/demo.css">
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/jstree/3.3.7/themes/default/style.min.css">
<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/jstree/3.3.7/jstree.min.js"></script>
<script src="/js/demo.js"></script>
</head>

<body>
<table>
<tr><td><a href="/portal"><img alt="logo" width="144" height="144" src="/img/logo.png"></a></td><tr>
</table>


<h1>List Files</h1>

<p/>

<table>
<tr class="gray">
    <td style=".owner">$addr.label</td>
    <td>$addr.address</td>
    <td>$addr.balance</td>
    <td><a href="/portal/rmaddr?addr=$addr.address">unregister</a></td>
</tr>
</table>

<form action="/portal/padd">
<input type="hidden" name="addr" value="$addr.address">
<input type="submit" value="Add Content">
</form>

<h2>IPFS files</h2>
<div id="treeIpfs"></div>

<h2>Local files</h2>
<div id="treeLocal"></div>

<script>
$(function() {

  $('#treeIpfs').jstree({
    'core' : $treeDataIpfs,
    'plugins': ["contextmenu"],
    "contextmenu": {         
        "items": treeMenuIpfs
    }
  });

  $('#treeLocal').jstree({
    'core' : $treeDataLocal,
    'plugins': ["contextmenu"],
    "contextmenu": {  
        "items": treeMenuLocal
    }
  });
});
</script>

</body>
</html>
